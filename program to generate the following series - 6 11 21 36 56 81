import java.util.*;
class Main
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
     int digit = 6;
     int diff = 5;
     System.out.println(digit + " ");
    for(int i=1;i<n;i++){
         digit += diff;
         diff += 5;
         System.out.println(digit + " ");
        }
  }
}

import java.util.*;
class Main
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int term = 0;
    int increment = 2;
     System.out.print(term + " ");
    for(int i=1;i<n;i++){
      term += increment;
      System.out.print(term + " ");
      if(i % 2 == 1){
        increment += 4;
      }
    }
  }
}

Sample Input:
6
Sample Output:
0 2 8 14 24 34
